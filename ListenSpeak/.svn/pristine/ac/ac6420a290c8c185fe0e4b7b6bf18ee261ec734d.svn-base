@charset "utf-8";
/* init */
* {
  border: none;
  margin: 0;
  padding: 0;
  font-size: 14px;
  color: #303030;
}
.hide {
  display: none;
}
.clearfix {
  clear: both;
}
/* login开始 */
.login {
  background-size: cover;
}
.login-top {
  width: 4.2rem !important;
  margin-left: auto;
  margin-right: auto;
  margin-top: 2.5rem;
  display: flex;
  display: -webkit-flex;
  align-items: center;
}
.login-top .logo {
  float: left;
  color: #64dcdc;
  border: 2px solid #64dcdc !important;
  -webkit-border-radius: 5px !important;
  -moz-border-radius: 5px !important;
  border-radius: 5px !important;
  width: 1rem !important;
  font-size: 0.35rem;
  text-align: center;
  line-height: 0.4rem;
  vertical-align: middle;
  box-sizing: border-box;
}
.login-top .login-ttiltle {
  float: left;
  margin-left: 0.1rem;
  font-size: 0.37rem;
  color: #ffffff;
  font-family: '方正兰亭粗黑';
  line-height: 0.5rem;
  font-weight: bold;
}
.login-main {
  width: 4.5rem !important;
  margin-left: auto;
  margin-right: auto;
  margin-top: 0.3rem;
}
.login-main > div {
  width: 100% !important;
  background: #ffffff;
  -webkit-border-radius: 5px !important;
  -moz-border-radius: 5px !important;
  border-radius: 5px !important;
  margin-top: 0.2rem;
}
.login-user,
.login-password {
  width: 100% !important;
  height: 0.6rem;
  display: flex;
  display: -webkit-flex;
  align-items: center;
}
.login-user img,
.login-password img {
  width: 0.3rem !important;
  float: left;
  padding: 0 0.1rem;
  border-right: 1px solid #e2e2e2;
}
.login-user input,
.login-password input {
  width: 3.8rem !important;
  height: 80%;
  margin-left: 0.1rem;
}
.login-button {
  width: 100% !important;
  background-color: rgba(0, 0, 0, 0) !important;
}
.login-button input {
  background: #64dcdc;
  color: #ffffff;
  line-height: 0.6rem;
  -webkit-border-radius: 5px !important;
  -moz-border-radius: 5px !important;
  border-radius: 5px !important;
  font-size: 0.18rem;
}
.login-button :nth-child(1) {
  width: 36% !important;
}
.login-button :nth-child(2) {
  margin-left: 3%;
  width: 60% !important;
}
.login-button p {
  color: #ffffff;
  margin-top: 0.1rem;
}
.login-bottom {
  position: fixed;
  bottom: 0.3rem;
  width: 100% !important;
  text-align: center;
}
/* login结束 */
/* 公共样式 */
.public-topbg {
  background: #f6fafc;
  border-bottom: 1px solid #ededed;
  line-height: 0.8rem;
}
