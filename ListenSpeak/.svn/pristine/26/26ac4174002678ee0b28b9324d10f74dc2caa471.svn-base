#questionContent{
	margin: 0;
    padding: 0;
    border: 0;
    font-style: normal;
    font-size: 100%;
    font: inherit;
    vertical-align: baseline;
    word-wrap: break-word;
}

.qc{
	width: 90%;
    height: auto;
    border: 1px solid #ddd;
    margin-top: 5px;
    margin-bottom: 11px;
    line-height: 34px;
    font-family: "simsun";
}

.qc p.info {
    height: 36px;
    line-height: 36px;
    font-size: 12px;
    border-bottom: 1px solid #ddd;
    font-family: simsun;
}

.qc .info {
    padding: 0 0px;
}

.color-grey6 {
    color: #666;
}

.bg-greyf5 {
    background-color: #f5f5f5;
}
.clearfix {
}
.qc .info span, .error {
    margin-right: 20px;
}

.red {
    color: #ff4402;
}

.padding15 {
    padding: 12px 15px;
    min-height: 120px;
}

.testbutton {
    font-family: "Microsoft YaHei";
}

.testbutton a {
	margin-top:4px;
    width: 96px;
    display: block;
    text-align: center;
    line-height:28px;
    font-size: 14px;
    color: #fff;
    border-radius: 2px;
    background: #249f59;
    display: inline;
    background: -webkit-linear-gradient(#249f59,#27ae61);
    background: -o-linear-gradient(#249f59,#27ae61);
    background: -moz-linear-gradient(#249f59,#27ae61);
    background: linear-gradient(#249f59,#27ae61);
}  

.fr {
    float: right;
}  

a {
    text-decoration: none;
}