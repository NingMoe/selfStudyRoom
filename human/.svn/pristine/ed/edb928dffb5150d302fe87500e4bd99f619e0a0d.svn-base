@media screen and (min-width:376px) and (max-width:414px){html{font-size:110px !important}}
@media screen and (min-width:415px) and (max-width:639px){html{font-size:110px !important}}
@media screen and (min-width:640px) and (max-width:719px){html{font-size:150px !important}}
@media screen and (min-width:720px) and (max-width:749px){html{font-size:150px !important}}
@media screen and (min-width:750px) and (max-width:799px){html{font-size:150px !important}}
@media screen and (min-width:800px){html{font-size:200px !important}}
