package com.human.recruitment.entity;

public class HrInterviewRecord {
    
    private Integer positiveId;
    
    private String interviewComment;
    
    private Integer interviewResult;
    
    private String refuseReason;

    public Integer getPositiveId() {
        return positiveId;
    }

    public void setPositiveId(Integer positiveId) {
        this.positiveId = positiveId;
    }

    public String getInterviewComment() {
        return interviewComment;
    }

    public void setInterviewComment(String interviewComment) {
        this.interviewComment = interviewComment;
    }

    public Integer getInterviewResult() {
        return interviewResult;
    }

    public void setInterviewResult(Integer interviewResult) {
        this.interviewResult = interviewResult;
    }

    public String getRefuseReason() {
        return refuseReason;
    }

    public void setRefuseReason(String refuseReason) {
        this.refuseReason = refuseReason;
    }
   
    
    
}
